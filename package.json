{
  "name": "gc-npm-package",
  "version": "0.1.7",
  "description": "一个智能的Git提交工具，帮助开发者快速、规范地提交代码，支持智能代码分析和标准化提交格式。仅支持macOS和Linux系统，Windows用户请使用WSL2或Git Bash。",
  "main": "gc",
  "bin": {
    "gc": "gc"
  },
  "scripts": {
    "test": "./test/simple-test.sh",
    "test:full": "./test/test-suite.sh",
    "test:package": "node test-package.js",
    "postinstall": "node postinstall.js"
  },
  "keywords": [
    "git",
    "commit",
    "conventional-commits",
    "cli",
    "tool",
    "development",
    "workflow",
    "bash",
    "shell",
    "automation",
    "productivity",
    "macos",
    "linux",
    "unix",
    "wsl"
  ],
  "author": "yichenzi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengqc1995/gc-npm-package.git"
  },
  "bugs": {
    "url": "https://github.com/mengqc1995/gc-npm-package/issues"
  },
  "homepage": "https://github.com/mengqc1995/gc-npm-package#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "platforms": [
    "macos",
    "linux",
    "wsl"
  ],
  "systemRequirements": {
    "macos": "需要Bash shell环境（系统默认支持）",
    "linux": "需要Bash shell环境（系统默认支持）",
    "windows": "不支持原生Windows，建议使用WSL2或Git Bash",
    "wsl": "在Windows Subsystem for Linux中完全支持"
  },
  "files": [
    "gc",
    "git-commit.sh",
    "code-analyzer.sh",
    "postinstall.js",
    "README.md",
    "test/"
  ],
  "bundledDependencies": [],
  "man": "./README.md"
}
